{
  "name": "cuttingedge",
  "version": "0.0.0",
  "description": "Discord Bot project for Cutting Edge Developers",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "clear": "node ./packages/cleaner/src/index.js",
